{
  "version": "2.0",
  "logging": {
    "applicationInsights": {
      "samplingSettings": {
        "isEnabled": true,
        "excludedTypes": "Exception;Request",
        "maxTelemetryItemsPerSecond": 1
      }
    },
    "logLevel": {
      "default": "Error",
      "Host.Results": "Error",
      "Host.Aggregator": "Information",
      "Function": "Error"
    }
  }
}